{
	"name": "vite-plugin-sveltekit-tex",
	"version": "0.0.5",
	"license": "MIT",
	"author": "Kelvin Soh",
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"description": "Generate LaTeX tex/pdf while building in SvelteKit",
	"homepage": "https://github.com/kelvinsjk/mathlified",
	"keywords": [
		"mathlified",
		"sveltekit-tex"
	],
	"files": [
		"dist"
	],
	"scripts": {
		"dev": "tsup --watch",
		"build": "tsup",
		"prepare": "tsup",
		"version": "pnpm format",
		"postversion": "git add . && git commit -m 'new version' && git push && git push --tags",
		"prepublishOnly": "pnpm lint",
		"preversion": "pnpm lint",
		"lint": "eslint . ",
		"format": "prettier --write --plugin-search-dir=. ."
	},
	"devDependencies": {
		"@types/command-exists": "^1.2.0",
		"@types/fs-extra": "^11.0.1",
		"@types/glob": "^8.0.1",
		"@typescript-eslint/eslint-plugin": "^5.48.2",
		"@typescript-eslint/parser": "^5.48.2",
		"eslint": "^8.32.0",
		"eslint-config-prettier": "^8.6.0",
		"prettier": "^2.8.3",
		"tsup": "^6.5.0",
		"typescript": "^4.9.4",
		"vite": "^4.0.4"
	},
	"dependencies": {
		"command-exists": "^1.2.9",
		"dependency-tree": "^9.0.0",
		"fs-extra": "^11.1.0",
		"glob": "^8.1.0",
		"kleur": "^4.1.5",
		"mathlifier": "workspace:^1.3.2",
		"mathlifier2": "workspace:^1.3.2",
		"node-latex": "^3.1.0",
		"outdent": "^0.8.0"
	},
	"peerDependencies": {
		"mathlifier": "^1.3.2",
		"mathlifier2": "^1.3.2",
		"fs-extra": "^11.1.0",
		"vite": "^4.0.4"
	}
}
